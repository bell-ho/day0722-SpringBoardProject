<insert id="insert">
  
  	<selectKey keyProperty="bno" order="BEFORE" resultType="long">
  		select seq_board.nextval from dual
  	</selectKey>
  
  	insert into tbl_board (bno,title,content,writer)
  	values (#{bno} , #{title},#{content},#{writer})
  	
  </insert>
  
